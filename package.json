{
  "name": "uvigothemewp",
  "version": "1.3.2",
  "author": "IdeiT <info@ideit.es>",
  "contributors": [
    "uvigo-atic <atic@uciugo.es>",
    "Lois Rodríguez <loisrodriguez@biela.gal>",
    "Fernando González <fernando.gonzalez@ideit.es>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uvigo/contrib-web-portalmp-wordpress-theme.git"
  },
  "bugs": {
    "url": "https://github.com/uvigo/contrib-web-portalmp-wordpress-theme/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --config resources/assets/build/webpack.config.js",
    "build:production": "webpack --env.production --progress --config resources/assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/assets/build/webpack.config.js",
    "start": "webpack --hide-modules --watch --config resources/assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint resources/assets/scripts resources/assets/build",
    "lint:styles": "stylelint \"resources/assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint",
    "deploy": "npm run build:production && node resources/assets/build/deploy.js",
    "pot": "mkdir -p ./languages && find ./views ./views_esei ./templates ./app -iname '*.php' | xargs /usr/local/opt/gettext/bin/xgettext --add-comments=TRANSLATORS --force-po --from-code=UTF-8 --default-domain=gl_ES -k__ -k_e -k_n:1,2 -k_x:1,2c -k_ex:1,2c -k_nx:4c,12 -kesc_attr__ -kesc_attr_e -kesc_attr_x:1,2c -kesc_html__ -kesc_html_e -kesc_html_x:1,2c -k_n_noop:1,2 -k_nx_noop:3c,1,2, -k__ngettext_noop:1,2 -o languages/esei.pot && find ./views ./views_esei ./templates -iname '*.blade.php' | xargs /usr/local/opt/gettext/bin/xgettext --language=Python --add-comments=TRANSLATORS --force-po --from-code=UTF-8 --default-domain=gl_ES -k__ -k_e -k_n:1,2 -k_x:1,2c -k_ex:1,2c -k_nx:4c,12 -kesc_attr__ -kesc_attr_e -kesc_attr_x:1,2c -kesc_html__ -kesc_html_e -kesc_html_x:1,2c -k_n_noop:1,2 -k_nx_noop:3c,1,2, -k__ngettext_noop:1,2 -j -o languages/esei.pot"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "~8.2.0",
    "bestzip": "^1.1.4",
    "browser-sync": "^2.26.7",
    "browsersync-webpack-plugin": "^0.6.0",
    "bs-html-injector": "~3.0",
    "buble-loader": "^0.4.1",
    "cache-loader": "~1.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "copy-globs-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "~4.0.5",
    "eslint": "~4.19.1",
    "eslint-loader": "~1.9",
    "eslint-plugin-import": "~2.14.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "~1.5",
    "node-sass": "~4.9.4",
    "postcss-loader": "~2.1.0",
    "postcss-safe-parser": "~3.0",
    "resolve-url-loader": "~2.3.1",
    "rimraf": "~2.6",
    "sass-loader": "~6.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "github:webpack-contrib/stylelint-webpack-plugin",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "~3.11.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-dev-middleware": "~2.0.4",
    "webpack-hot-middleware": "~2.22.3",
    "webpack-merge": "~4.1.4",
    "yargs": "~11.0.0"
  },
  "dependencies": {
    "contrib-web-portalmp-frontend": "github:uvigo/contrib-web-portalmp-frontend#SXIADIE5-5",
    "font-awesome": "~4.7",
    "jquery": "1.12.4 - 3"
  }
}
